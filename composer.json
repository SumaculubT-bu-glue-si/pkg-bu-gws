{
    "name": "bu/gws",
    "description": "Business Unit GWS Package",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
        "Bu\\Gws\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bu\\Gws\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
        "type": "vcs",
        "url": "https://github.com/SumaculubT-bu-glue-si/bu-dal-package.git"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0 || ^11.0 || ^12.0",
        "google/apiclient": "^2.0",
        "nuwave/lighthouse": "^6.0",
        "bu/server": "dev-main"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bu\\Gws\\Providers\\GwsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}